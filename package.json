{
  "name": "pricing-driven-feature-toggling-demo",
  "type": "module",
  "version": "1.0.0",
  "description": "This is a demo project that uses pricing-driven feature flags driven by different solutions",
  "scripts": {
    "load-test:tomatometer-renew-only": "artillery run ./artillery/tests/tomatometer-renew-only.yml --name \"tomatometer-renew-only\" --record --key a9_koizwzxadtywh677t9hwg9pvogmr600f",
    "load-test:space-renew-only": "artillery run ./artillery/tests/space-renew-only.yml --name \"space-renew-only\" --record --key a9_koizwzxadtywh677t9hwg9pvogmr600f",
    "load-test:tomatometer-validate-only": "artillery run ./artillery/tests/tomatometer-validate-only.yml --name \"tomatometer-validate-only\" --record --key a9_koizwzxadtywh677t9hwg9pvogmr600f",
    "load-test:space-validate-only": "artillery run ./artillery/tests/space-validate-only.yml --name \"space-validate-only\" --record --key a9_koizwzxadtywh677t9hwg9pvogmr600f",
    "load-test": "pnpm run load-test:server-feature-validation && pnpm run load-test:renew-token",
    "dev:server": "export SPACE_URL=\"http://localhost:3000\" && npx tsx --watch ./api/backend.ts",
    "dev:client": "concurrently \"vite\" \"tailwindcss -i ./src/static/css/app.css -o ./src/static/css/tailwind.css --watch\"",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "vite build",
    "build:server": "tsc -p ./tsconfig.json && tsc-alias --project=\"./tsconfig.json\" --resolve-full-paths",
    "build": "npm run build:server && npm run build:client "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alex-GF/space-demo.git"
  },
  "author": "Alejandro García Fernández",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alex-GF/space-demo/issues"
  },
  "homepage": "https://github.com/Alex-GF/space-demo#readme",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "@iconify/react": "^5.0.2",
    "@types/prop-types": "^15.7.13",
    "@types/react-helmet": "^6.1.11",
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "framer-motion": "^12.12.2",
    "helmet": "^8.0.0",
    "js-yaml": "^4.1.0",
    "pricing4ts": "^0.8.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-svg": "^16.1.34",
    "space-node-client": "^0.3.0",
    "space-react-client": "^0.2.3",
    "tsc-alias": "1.8.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/artillery": "^1.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "artillery": "^2.0.23",
    "concurrently": "^9.1.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-plugin-node-polyfills": "^0.22.0"
  }
}
